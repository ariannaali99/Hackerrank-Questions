class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        //Initializing variables
        int original_length = nums.size();
        int duplicates = 0;
        int new_length = 0;
        
        int i = 0;
        int j = 0;
        for(i = 0; i < original_length; i++) {
            //At the first element, we do nothing because it is automatically unique
            if(i > 0) {
                //checking to see if it is an obsolete number
                if(nums[i] == -200) {
                    nums[i] = nums[i+1];
                    nums[i+1] = -200;
                }
                
                for(j = 0; j < i; j++) {
                    if(nums[i] == nums[j]) {
                        //Changing the number to one that does not exist in our data
                        nums[i] = nums[i + 1];
                        nums[i+1] = -200;
                        duplicates += 1;
                        //We want to re-evaluate the same element at i
                        i = i - 1;
                    }
                    //else do nothing
                }
            }
        }
        new_length = original_length - duplicates;
        return new_length;
    }
};
